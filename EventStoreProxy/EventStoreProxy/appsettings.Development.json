{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Yarp": "Information"
      }
    }
  },
  "ClientCerts": "esdb-client-certs",
  "EventStore": {
    "ClusterUrls": "https://localhost:30000/",
    "Certificates": {
      "BasePath": "certs"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:6870/"
      },
      "Https": {
        "Url": "https://localhost:7119/"
      }
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "backend": {
        "Destinations": {
          "esdb": {
            "Address": "https://localhost:3000/"
          }
        }
      }
    }
  }
}
